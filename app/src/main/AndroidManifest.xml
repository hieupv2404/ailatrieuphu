<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.group7.com.altp">

    <application
        android:name="com.group7.com.altp.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.group7.com.altp.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.group7.com.altp.activity.ActivityLevel"/>
        <activity android:name="com.group7.com.altp.activity.ActivityPlayGame"/>
        <activity android:name="com.group7.com.altp.activity.HightScoreActivity"></activity>
        
    </application>

</manifest>